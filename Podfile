# platform :ios, '9.0'

target 'TravelEssay' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!
  
  # Pod for Network Communication
  pod 'Alamofire', '~> 5.2'
  
  ## Firebase ðŸ”¥ pods
  pod 'Firebase/Auth'
  pod 'Firebase/Firestore'
  
